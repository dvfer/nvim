syntax on
set number relativenumber
set mouse=a
set clipboard=unnamedplus
set nocompatible
set ruler
set showmatch
set laststatus=2
set cursorline
set shiftwidth=4
set tabstop=4
set autoindent
set expandtab
filetype indent on
hi MatchParen cterm=bold ctermbg=none ctermfg=magenta

set title
set titlestring=%{hostname()}\ \ %F\ 
set undofile

